{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "require-jsdoc": "off",
        "indent": [2, 4],
        "arrow-parens": "off",
        "brace-style": ["error", "stroustrup"],
        "comma-dangle": "off",
        "keyword-spacing": "off",
        "max-len": "off"
    }
}
